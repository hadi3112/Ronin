<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="gettingStartedTwoVM" type="com.muhammadsapplication.app.modules.gettingstartedtwo.data.viewmodel.GettingStartedTwoVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearColumngettingstartedOne"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylegray_800">
        <TextView android:ellipsize="end"
            android:gravity="center_horizontal"
            android:id="@+id/txtGettingStartedOne"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_14pxh"
            android:layout_marginStart="@dimen/_14pxh"
            android:layout_marginTop="@dimen/_52pxv"
            android:layout_width="match_parent"
            android:singleLine="true"
            android:text="@{gettingStartedTwoVM.gettingStartedTwoModel.txtGettingStartedOne}"
            style="@style/txtPoppinsbold24" tools:text="@string/lbl_getting_started"/>
        <FrameLayout android:id="@+id/frameStackchoosewhatto"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_578pxv"
            android:layout_marginBottom="@dimen/_16pxv"
            android:layout_marginEnd="@dimen/_14pxh"
            android:layout_marginStart="@dimen/_14pxh"
            android:layout_marginTop="@dimen/_16pxv" android:layout_width="@dimen/_332pxh">
            <FrameLayout android:id="@+id/frameStackcontrast"
                android:layout_gravity="bottom|center"
                android:layout_height="@dimen/_18pxv"
                android:layout_marginBottom="@dimen/_43pxv"
                android:layout_marginEnd="@dimen/_137pxh"
                android:layout_marginStart="@dimen/_137pxh"
                android:layout_marginTop="@dimen/_43pxv" android:layout_width="@dimen/_58pxh">
                <ImageView android:id="@+id/imageContrast"
                    android:layout_gravity="start"
                    android:layout_height="@dimen/_18pxh"
                    android:layout_width="@dimen/_58pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_contrast"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_contrast"/>
                <ImageView android:id="@+id/imageContrastOne"
                    android:layout_gravity="start"
                    android:layout_height="@dimen/_18pxh"
                    android:layout_width="@dimen/_58pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_contrast_bluegray_100"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_contrast_bluegray_100"/>
            </FrameLayout>
            <LinearLayout android:id="@+id/linearInfoCard"
                android:layout_gravity="top|center"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_10pxv"
                android:layout_marginEnd="@dimen/_21pxh"
                android:layout_marginStart="@dimen/_21pxh"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylered_402cornerRadius">
                <TextView android:ellipsize="end"
                    android:gravity="center_horizontal"
                    android:id="@+id/txtChoosewhatto"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_24pxh"
                    android:layout_marginStart="@dimen/_24pxh"
                    android:layout_marginTop="@dimen/_17pxv"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@{gettingStartedTwoVM.gettingStartedTwoModel.txtChoosewhatto}"
                    style="@style/txtPoppinsbold20" tools:text="@string/msg_choose_what_to"/>
                <LinearLayout android:id="@+id/linearColumncourseicon"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_84pxv"
                    android:layout_marginEnd="@dimen/_21pxh"
                    android:layout_marginStart="@dimen/_24pxh"
                    android:layout_marginTop="@dimen/_30pxv"
                    android:layout_width="match_parent"
                    android:orientation="vertical" style="@style/groupStylered_300cornerRadius">
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearRowcourseicon"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginStart="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_12pxv"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageCourseicon"
                            android:layout_height="@dimen/_26pxh"
                            android:layout_marginTop="@dimen/_2pxv"
                            android:layout_width="@dimen/_28pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_kindpng2082370"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_kindpng2082370"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtLanguage"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_5pxv"
                            android:layout_marginStart="@dimen/_8pxh"
                            android:layout_weight="0.83"
                            android:layout_width="0dp"
                            android:singleLine="true"
                            android:text="@{gettingStartedTwoVM.gettingStartedTwoModel.txtLanguage}"
                            style="@style/txtPoppinsbold15" tools:text="@string/msg_pick_a_prefere"/>
                    </LinearLayout>
                    <TextView android:gravity="center_horizontal"
                        android:id="@+id/txtSelectwhattec"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginStart="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_2pxv"
                        android:layout_width="match_parent"
                        android:text="@{gettingStartedTwoVM.gettingStartedTwoModel.txtSelectwhattec}"
                        style="@style/txtPoppinsregular10" tools:text="@string/msg_select_what_tec"/>
                    <FrameLayout
                        android:id="@+id/frameStackicons8deadline"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_172pxv"
                        android:layout_marginTop="@dimen/_14pxv" android:layout_width="@dimen/_245pxh">
                        <LinearLayout
                            android:id="@+id/linearRowicons8deadline"
                            android:layout_gravity="top|start"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_10pxv"
                            android:layout_width="match_parent"
                            android:orientation="horizontal" style="@style/groupStylered_200cornerRadius">
                            <ImageView
                                android:id="@+id/imageIcons8deadline"
                                android:layout_height="@dimen/_36pxh"
                                android:layout_marginBottom="@dimen/_53pxv"
                                android:layout_marginStart="@dimen/_7pxh"
                                android:layout_marginTop="@dimen/_4pxv"
                                android:layout_width="@dimen/_36pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_icons8deadline"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_icons8deadline"/>
                            <LinearLayout android:gravity="start"
                                android:id="@+id/linearColumndonotworry"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/_22pxv"
                                android:layout_marginEnd="@dimen/_19pxh"
                                android:layout_marginStart="@dimen/_7pxh"
                                android:layout_marginTop="@dimen/_11pxv"
                                android:layout_weight="0.83"
                                android:layout_width="0dp" android:orientation="vertical">
                                <TextView android:ellipsize="end"
                                    android:id="@+id/txtDonotworry"
                                    android:layout_height="wrap_content"
                                    android:layout_marginEnd="@dimen/_10pxh"
                                    android:layout_width="match_parent"
                                    android:singleLine="true"
                                    android:text="@{gettingStartedTwoVM.gettingStartedTwoModel.txtDonotworry}"
                                    style="@style/txtPoppinsbold15" tools:text="@string/lbl_do_not_worry"/>
                                <TextView
                                    android:id="@+id/txtDescription"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="@dimen/_5pxv"
                                    android:layout_width="match_parent"
                                    android:text="@{gettingStartedTwoVM.gettingStartedTwoModel.txtDescription}"
                                    style="@style/txtPoppinsregular10" tools:text="@string/msg_there_s_tutoria"/>
                            </LinearLayout>
                        </LinearLayout>
                        <LinearLayout
                            android:id="@+id/linearRowicons8behaviou"
                            android:layout_gravity="bottom|start"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_10pxv"
                            android:layout_width="match_parent"
                            android:orientation="horizontal" style="@style/groupStylered_201cornerRadius">
                            <ImageView
                                android:id="@+id/imageIcons8behaviou"
                                android:layout_height="@dimen/_28pxh"
                                android:layout_marginBottom="@dimen/_50pxv"
                                android:layout_marginStart="@dimen/_11pxh"
                                android:layout_marginTop="@dimen/_8pxv"
                                android:layout_width="@dimen/_28pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_icons8behaviou"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_icons8behaviou"/>
                            <LinearLayout
                                android:id="@+id/linearColumnexplore"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/_15pxv"
                                android:layout_marginEnd="@dimen/_23pxh"
                                android:layout_marginStart="@dimen/_8pxh"
                                android:layout_marginTop="@dimen/_10pxv"
                                android:layout_weight="0.86"
                                android:layout_width="0dp" android:orientation="vertical">
                                <TextView android:ellipsize="end"
                                    android:id="@+id/txtExplore"
                                    android:layout_gravity="start"
                                    android:layout_height="wrap_content"
                                    android:layout_marginEnd="@dimen/_10pxh"
                                    android:layout_marginStart="@dimen/_6pxh"
                                    android:layout_width="wrap_content"
                                    android:singleLine="true"
                                    android:text="@{gettingStartedTwoVM.gettingStartedTwoModel.txtExplore}"
                                    style="@style/txtPoppinsbold15" tools:text="@string/lbl_explore"/>
                                <TextView android:gravity="start"
                                    android:id="@+id/txtDescriptionOne"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="@dimen/_5pxv"
                                    android:layout_width="match_parent"
                                    android:text="@{gettingStartedTwoVM.gettingStartedTwoModel.txtDescriptionOne}"
                                    style="@style/txtPoppinsregular10" tools:text="@string/msg_go_through_as_m"/>
                            </LinearLayout>
                        </LinearLayout>
                    </FrameLayout>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearMascot"
                android:layout_gravity="bottom|start"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <FrameLayout android:id="@+id/frameStackspeechbubble"
                    android:layout_height="@dimen/_91pxv"
                    android:layout_marginBottom="@dimen/_77pxv"
                    android:layout_width="@dimen/_255pxh" style="@style/groupStylecornerRadius">
                    <ImageView android:id="@+id/imageSpeechbubble"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_91pxh"
                        android:layout_width="match_parent"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_speechbubble"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_speechbubble"/>
                    <TextView android:gravity="top|center"
                        android:id="@+id/txtHiLookingto"
                        android:layout_gravity="top|center"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_12pxv"
                        android:layout_marginEnd="@dimen/_11pxh"
                        android:layout_marginStart="@dimen/_11pxh"
                        android:layout_marginTop="@dimen/_12pxv"
                        android:layout_width="match_parent"
                        android:text="@{gettingStartedTwoVM.gettingStartedTwoModel.txtHiLookingto}"
                        style="@style/txtPoppinsregular15" tools:text="@string/msg_hi_looking_to"/>
                </FrameLayout>
                <ImageView android:id="@+id/imageMascotimg"
                    android:layout_height="@dimen/_122pxh"
                    android:layout_marginTop="@dimen/_46pxv"
                    android:layout_width="@dimen/_77pxh"
                    android:scaleType="fitXY"
                    imageRes="@{@drawable/img_seekpng1}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_seekpng1"/>
            </LinearLayout>
        </FrameLayout>
    </LinearLayout>
</layout>
