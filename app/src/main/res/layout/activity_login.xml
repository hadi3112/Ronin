<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="loginVM" type="com.muhammadsapplication.app.modules.login.data.viewmodel.LoginVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearColumnseekpngone"
        android:layout_height="match_parent"
        android:layout_marginTop="@dimen/_19pxv"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylered_400">
        <androidx.appcompat.widget.Toolbar
            android:gravity="center_horizontal"
            android:id="@+id/toolbarToolbar"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_17pxh"
            android:layout_marginStart="@dimen/_22pxh"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <LinearLayout android:id="@+id/linearRowwelcome"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <LinearLayout android:id="@+id/linearColumnwelcome"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.76"
                    android:layout_width="0dp" android:orientation="vertical">
                    <TextView android:ellipsize="end"
                        android:gravity="start"
                        android:id="@+id/txtWelcome"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{loginVM.loginModel.txtWelcome}"
                        style="@style/txtPoppinsbold32" tools:text="@string/lbl_welcome"/>
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearRowkindpng1595887"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginStart="@dimen/_5pxh"
                        android:layout_marginTop="@dimen/_14pxv"
                        android:layout_width="wrap_content" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageKindpng1595887"
                            android:layout_height="@dimen/_38pxh"
                            android:layout_marginBottom="@dimen/_6pxv"
                            android:layout_marginTop="@dimen/_21pxv"
                            android:layout_width="@dimen/_38pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_kindpng1595887"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_kindpng1595887"/>
                        <LinearLayout
                            android:id="@+id/linearColumnpngitem131193"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_2pxh"
                            android:layout_width="match_parent" android:orientation="vertical">
                            <LinearLayout
                                android:id="@+id/linearRowpngitem131193"
                                android:layout_height="wrap_content"
                                android:layout_width="match_parent" android:orientation="horizontal">
                                <ImageView
                                    android:id="@+id/imagePngItem131193"
                                    android:layout_height="@dimen/_25pxh"
                                    android:layout_marginBottom="@dimen/_7pxv"
                                    android:layout_width="@dimen/_25pxh"
                                    android:scaleType="fitXY"
                                    android:src="@drawable/img_pngitem131193"
                                    tools:ignore="ContentDescription" tools:src="@drawable/img_pngitem131193"/>
                                <TextView android:ellipsize="end"
                                    android:id="@+id/txtC"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="@dimen/_15pxh"
                                    android:layout_marginTop="@dimen/_8pxv"
                                    android:layout_weight="0.58"
                                    android:layout_width="0dp"
                                    android:singleLine="true"
                                    android:text="@{loginVM.loginModel.txtC}"
                                    style="@style/txtPoppinsbold16" tools:text="@string/lbl_c"/>
                            </LinearLayout>
                            <ImageView
                                android:id="@+id/imageKindpng2082370"
                                android:layout_gravity="start"
                                android:layout_height="@dimen/_26pxh"
                                android:layout_marginEnd="@dimen/_10pxh"
                                android:layout_marginStart="@dimen/_9pxh"
                                android:layout_marginTop="@dimen/_7pxv"
                                android:layout_width="@dimen/_28pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_kindpng2082370"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_kindpng2082370"/>
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
                <ImageView android:id="@+id/imageSeekPngOne"
                    android:layout_height="@dimen/_122pxh"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginTop="@dimen/_5pxv"
                    android:layout_width="@dimen/_77pxh"
                    android:scaleType="fitXY"
                    imageRes="@{@drawable/img_seekpng1}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_seekpng1"/>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearColumn"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_32pxv"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylered_400">
            <LinearLayout android:gravity="center_horizontal"
                android:id="@+id/linearColumnemail"
                android:layout_height="match_parent"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylebluegray_50cornerRadius">
                <LinearLayout android:id="@+id/linearColumnlogin"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="match_parent"
                    android:orientation="vertical" style="@style/groupStylewhite_A700cornerRadius">
                    <LinearLayout android:id="@+id/linearRowlogin"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_58pxh"
                        android:layout_marginStart="@dimen/_58pxh"
                        android:layout_marginTop="@dimen/_11pxv"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtLogin"
                            android:layout_height="wrap_content"
                            android:layout_weight="0.41"
                            android:layout_width="0dp"
                            android:singleLine="true"
                            android:text="@{loginVM.loginModel.txtLogin}"
                            style="@style/txtPoppinsregular16" tools:text="@string/lbl_login"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtSignUp"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_140pxh"
                            android:layout_weight="0.59"
                            android:layout_width="0dp"
                            android:singleLine="true"
                            android:text="@{loginVM.loginModel.txtSignUp}"
                            style="@style/txtPoppinsregular16_1" tools:text="@string/lbl_sign_up"/>
                    </LinearLayout>
                    <View
                        android:background="@drawable/rectangle_bg_red_401_radius_tl_20_tr_20"
                        android:id="@+id/lineSelector"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_5pxv"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_3pxv" android:layout_width="@dimen/_160pxh"/>
                </LinearLayout>
                <androidx.recyclerview.widget.RecyclerView
                    android:gravity="center_horizontal"
                    android:id="@+id/recyclerLogin"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_27pxh"
                    android:layout_marginStart="@dimen/_27pxh"
                    android:layout_marginTop="@dimen/_46pxv"
                    android:layout_width="match_parent"
                    android:orientation="vertical"
                    app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                    itemSpace="@{@dimen/_16pxh}" tools:itemCount="2" tools:listitem="@layout/row_login"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtForgotPassword"
                    android:layout_gravity="end"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_27pxh"
                    android:layout_marginStart="@dimen/_27pxh"
                    android:layout_marginTop="@dimen/_9pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{loginVM.loginModel.txtForgotPassword}"
                    style="@style/txtPoppinsregular14" tools:text="@string/lbl_forgot_password"/>
                <androidx.appcompat.widget.AppCompatButton
                    android:ellipsize="end" android:gravity="center"
                    android:id="@+id/btnSignIn"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_27pxh"
                    android:layout_marginStart="@dimen/_27pxh"
                    android:layout_marginTop="@dimen/_25pxv"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_8pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_30pxh"
                    android:paddingTop="@dimen/_8pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_sign_in"
                    android:textAllCaps="false"
                    style="@style/btnSolidRounded" tools:text="@string/lbl_sign_in"/>
                <LinearLayout android:gravity="center_vertical"
                    android:id="@+id/linearRowgooglefab"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_144pxv"
                    android:layout_marginEnd="@dimen/_27pxh"
                    android:layout_marginStart="@dimen/_27pxh"
                    android:layout_marginTop="@dimen/_44pxv"
                    android:layout_width="wrap_content" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageGooglefab"
                        android:layout_height="@dimen/_41pxh"
                        android:layout_marginTop="@dimen/_4pxv"
                        android:layout_width="@dimen/_41pxh"
                        android:scaleType="centerCrop"
                        circular="@{true}"
                        imageRes="@{@drawable/img_googlefab}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_googlefab"/>
                    <ImageView android:id="@+id/imageFacebookFab"
                        android:layout_height="@dimen/_41pxh"
                        android:layout_marginStart="@dimen/_25pxh"
                        android:layout_marginTop="@dimen/_4pxv"
                        android:layout_width="@dimen/_41pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_facebookfab"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_facebookfab"/>
                    <ImageView android:id="@+id/imageApplefab"
                        android:layout_height="@dimen/_45pxh"
                        android:layout_marginStart="@dimen/_23pxh"
                        android:layout_width="@dimen/_45pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_applefab"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_applefab"/>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
